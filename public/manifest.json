{
  "name": "Mariana Matheos Jazz Band",
  "short_name": "Mariana Jazz",
  "description": "Banda de jazz, soul e R&B para eventos em BH. Música ao vivo com elegância e presença cênica.",
  "start_url": "/?source=pwa",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "background_color": "#000000",
  "theme_color": "#800000",
  "orientation": "portrait-primary",
  "scope": "/",
  "id": "/",
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["focus-existing", "auto"]
  },
  "edge_side_panel": {
    "preferred_width": 480
  },
  "protocol_handlers": [
    {
      "protocol": "mailto",
      "url": "/contato?mail=%s"
    }
  ],
  "capture_links": "existing-client-navigate",
  "lang": "pt-BR",
  "categories": ["music", "entertainment", "business"],
  "shortcuts": [
    {
      "name": "Agendar Evento",
      "short_name": "Agendar",
      "description": "Agende um show com a banda",
      "url": "/eventos?source=pwa",
      "icons": [{ "src": "/favicon/favicon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "Repertório",
      "short_name": "Músicas",
      "description": "Veja nosso repertório",
      "url": "/repertorio?source=pwa",
      "icons": [{ "src": "/favicon/favicon-192x192.png", "sizes": "192x192" }]
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
